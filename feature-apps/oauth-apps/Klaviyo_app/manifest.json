{
  "platform-version": "1.0",
  "product": {
    "surveysparrow": {
      "location": {
        "full_page_app": {
          "url": "index.html",
          "icon": "icon.svg"
        }
      },
      "functions": {
        "addContact":{
          "timeout": 10
        },
        "fetchContacts":{
          "timeout": 10
        }
      },
      "events":{
        "onSubmissionComplete":{
          "handler": "onSubmissionComplete"
        }
      }
    }
  },
  "whitelisted-domains": [
    "https://(.*)surveysparrow.com(.*)",
    "https://(.*).klaviyo.com(.*)",
    "https://(.*).us-east-1.on.aws/"
  ],
  "engines": {
    "node": "18.20.6",
    "ssdk": "0.3.8"
  }
}
